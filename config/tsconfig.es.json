{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES2015",
    "composite": true,
    "lib": ["ES5", "dom", "ES2015.Promise"],
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "declaration": true,
    "types": []
  }
}
